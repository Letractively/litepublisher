body{color:#%%body%%;background:#%%bodybackground%%} 
a,a:link,a:visited{color:#%%link%%}
#content a,#content a:link,#content a:visited{color:#%%linkcontent%%}
a:hover,#content a:hover{color:#%%hoverlink%%}
h1,h2,h3,h4,h5,h6{color:#%%h%%}
#top {background:url(%%headerurl%%) repeat-x top center #%%header%%;}
a#logo{background:url(%%logourl%%) no-repeat scroll 0 0 transparent;color:#%%logo%%}

#nav a{color:#%%menu%%}
#nav a:hover,#nav li.jshover a{background:#%%menubackground%%;color:#%%menuhover%%}
#nav li a.current_page_link{background:#%%currentmenubackground%%;color:#%%currentmenu%%}
#nav li a.current_page_link:hover,#nav li.jshover a.current_page_link{background:#%%hovercurrentmenubackground%%;color:#%%hovercurrentmenu%%}
#nav li ul li a,#nav li.jshover ul li a{background:#%%submenubackground%%;color:#%%submenu%%}
#nav li ul li a:hover,#nav li ul li.jshover,#nav li.jshover ul li a:hover{background:#%%hoversubmenubackground%%;color:#%%hoversubmenu%%}

#content{background:#%%contentbackground%%}

#wrapper{background:#%%sidebarbackground%%}
.sbar{color:#%%sidebar%%}
.sbar h3{color:#%%headsidebar%%}
#btm{background:#%%footerbackground%%;border-top:1px solid #%%footerborder%%;color:#%%footer%%}
