<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ru">
<head>
	<title><!--title-->$title | $options.name<!--/title--></title>
	<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
	<link rel="stylesheet" href="$template.url/css/style.css" type="text/css" media="all" />
	<link rel="alternate" type="application/rss+xml" title="$options.name RSS Feed" href="$options.url/rss.xml" />
	<link rel="pingback" href="$options.url/rpc.xml" />
	 <link rel="EditURI" type="application/rsd+xml" title="RSD" href="$options.url/rsd.xml" />
	<link rel="wlwmanifest" type="application/wlwmanifest+xml" href="$options.url/wlwmanifest.xml" />
	<link rel="meta" type="application/rdf+xml" title="FOAF" href="$options.url/foaf.xml" />
	<link rel="shortcut icon" type="image/x-icon" href="$template.icon" />
	<meta name="generator" content="Lite Publisher $options.version" /> <!-- leave this for stats -->
	<meta name="keywords" content="$template.keywords" />
	<meta name="description" content="$template.description" />
	<link rel="sitemap" href="$options.url/sitemap.htm" />
	<link rel="openid.server" href="$options.url/openid/" />
	<link rel="openid2.provider" href="$options.url/openid/" />
	<link rel="openid.delegate" href="$options.url/openid/" />
	<link rel="openid2.local_id" href="$options.url/openid/" />
	<!--[if IE 6]>
		<script src="$template.url/css/DD_belatedPNG.js"></script>
		<script>		
		  DD_belatedPNG.fix('.rss, .rss-comments, .foaf, #lastcomments li, #lastposts li, .post-comments, .post-cat, .post-time, .post-tags, #header');
		 </script>
	<![endif]--> 
	<script type="text/javascript" src="$options.files/js/litepublisher/rpc.min.js"></script>
	<script type="text/javascript" src="$options.files/js/litepublisher/client.min.js"></script>
	<script type="text/javascript" src="$options.files/js/jsibox/jsibox_basic.js"></script>
	$template.head
</head>
<body>
	<div id="header"><div id="logo"><span class="name"><a href="$options.url/">$options.name</a></span><span class="slogan">$options.description</span>
</div></div>
	<div id="topmenu">
	<!--menulist-->
	<ul id="nav">
		<!--item-->
		<li><a href="$options.url$url" title="$title">$title</a><!--submenu--><ul>$items</ul><!--/submenu--></li>
		<!--/item-->
		<!--current-->
		<li class="current_page_item"><a href="$options.url$url" title="$title">$title</a>$submenu</li>
		<!--/current-->	
	</ul>
	<!--/menulist-->
</div>
<div id="out">
<div id="middle">
	<div id="content">
		<!--content-->
		
			<!--simple-->
			<div class="simplecontent">$content</div>
			<!--/simple-->
		
			<!--notfound-->
			<div class="simplecontent">
				<h2 class="center">$lang.notfound </h2>
				<p class="center">$lang.nocontent</p>
			</div>
			<!--/notfound-->
		
		<!--excerpts-->
		<!--excerpt-->
		<div class="post" id="post-$post.id">
			<h2><a href="$post.link" rel="bookmark" title="$lang.permalink $post.title">$post.iconlink$post.title</a></h2>
				<span class="post-date"><!--date-->d.m.Y<!--/date--></span>
				<span class="post-tags">$post.tagslinks</span>
			<div class="post-content">$post.excerpt 
				<!--more--> <a href="$options.url$post.url#more-$post.id" class="more-link">$post.moretitle</a><!--/more-->
				$post.excerptfilelist
			</div>
			<div class="post-info">
				<span class="post-comments">$post.commentslink</span>
				<span class="post-cat">$post.categorieslinks</span>	
			</div>
		</div>
		<!--/excerpt-->

			<!--lite-->
			<h1>$lang.posts</h1>
			<ul>
				<!--excerpt-->
				<li>$post.date <a href="$options.url$post.url" title="$lang.permalink $post.title">$post.title</a></li>
				<!--/excerpt-->
			</ul>
			<!--/lite-->

		<!--/excerpts-->

		<!--post-->
			<div class="post" id="post-$post.id">
		        <h2><a href="$post.link" rel="bookmark" title="$lang.permalink $post.title">$post.iconlink$post.title</a></h2>
					<span class="post-date"><!--date-->d.m.Y<!--/date--></span>
					<span class="post-tags">						<!--tags-->
							$lang.tags: 
								<!--item-->
								<a href="$options.url$url" title="$title">$icon$title</a>
								<!--/item-->
								<!--divider-->, <!--/divider-->
													<!--/tags--></span>
						
												<div class="post-content">
							<!--files-->
								<ul id="files-$post.id">
								<!--file--><li><a href="$options.files/files/$filename" title="$title">$description</a></li><!--/file-->
								<!--image--><li><a rel="gale-$post.id" onclick="return jsiBoxOpen(this)" href="$options.files/files/$filename" title="$title">$preview$title</a></li><!--/image-->
<!--preview--><img src="$options.files/files/$preview.filename" title="$title" alt="$title" /><!--/preview-->
								<!--audio-->
								<li><a href="$options.files/files/$filename" title="$title">$title</a>
								<span id="audiofile-$id"><a onclick="playaudiofile('audiofile-$id', '$options.files/files/$filename');" title="$title">$lang.play</a></span>
								</li><!--/audio-->
								<!--video-->
								<li><a href="$options.files/files/$filename" title="$title" id="videofile-$id">$title</a>
<a onclick="playvideofile('videofile-$id', '$options.files/files/$filename');" title="$title">$lang.play</a>
								</li><!--/video-->
								</ul>
							<!--/files-->
							<!--more--><a name="more-$post.id"></a><!--/more-->
							$post.content
							
							<div class="post-info">
								<!--rss--><span class="post-rsscomments"><a href="$post.rsscomments" title="$lang.subscribetorsscomments" >$lang.subscribetorsscomments</a></span><!--/rss-->
								<span class="post-cat">						<!--commontags-->
							$lang.categories: 
								<!--item-->
								<a href="$options.url$url" title="$title">$icon$title</a>
								<!--/item-->
								<!--divider-->, <!--/divider-->
													<!--/categories--></span>	
							</div>
						
							<!--prevnext-->
								<div id="prevnext">
								<!--prev--><p id="prev">&larr; $lang.prev <a rel="prev" href="$prevpost.link" title="$lang.permalink $prevpost.title">$prevpost.title</a></p><!--/prev-->
								<!--next--><p id="next">$lang.next <a rel="next" href="$nextpost.link" title="$lang.permalink $nextpost.title">$nextpost.title</a> &rarr;</p><!--/next-->
								</div>
							<!--/prevnext-->
						</div>
						
						<!--templatecomments-->
							<!--comments-->
								<!--count-->
								<p id="comments">$count $lang.topost &#8220;$post.title&#8221;</p> 
								<!--/count-->
									<!--hold--><p class="hold"><em>$lang.hold</em></p><!--/hold-->
									   <a name="comments"></a>
										<ol class="commentlist" id="<!--commentsid-->commentlist<!--/commentsid-->" start="$from">
											<!--comment--><li <!--class1-->class="alt"<!--/class1--> id="comment-$comment.id">
													<a name="comment-$comment.id"></a>
														<div class="commentmetadata"><a href="#comment-$comment.id" title=""><!--date-->d.m.Y<!--/date--> $lang.attime $comment.time</a></div>
														<span class="author"><cite>$comment.authorlink</cite> $lang.says:  </span>
														
														<!--moderate-->
															<div class="moderationbuttons">
																<input type="checkbox" name="checkbox-comment-$comment.id" id="checkbox-comment-$comment.id" value="$comment.id" />
																<input type="button" value="$lang.approve" onclick="setcommentstatus($comment.id, 'approved');" />
																<input type="button" value="$lang.holdbutton" onclick="setcommentstatus($comment.id, 'hold');" />
																<input type="button" value="$lang.delete" onclick="deletecomment($comment.id);" />
																<input type="button" value="$lang.edit" onclick="editcomment($comment.id);" />
															</div>
														<!--/moderate-->
														
														<p id="commentcontent-$comment.id">
															$comment.content
														</p>
														<div class="replyquotebuttons">
															<input type="button" value="$lang.reply" onclick="replycomment($comment.id,'$comment.name');" />
															<input type="button" value="$lang.quote" onclick="quotecomment($comment.id,'$comment.name');" />
														</div>
												</li><!--/comment-->
										</ol>
							<!--/comments-->
							<!--moderateform-->
								<form name="moderateform" action="$options.url/admin/comments/" method="post">
								$comments
								<p>
								<input type="button" name="approvebutton" value="$lang.approve" onclick="submitmoderateform(this.form, 'approved');" />
								<input type="button" name="deletebutton" value="$lang.delete" onclick="submitmoderateform(this.form, 'delete');" />
								<input type="button" name="holdbutton" value="$lang.holdbutton" onclick="submitmoderateform(this.form, 'hold');" />
								<input type="button" value="$lang.sendreply" onclick="sendreply();" />
								</p>
								</form>
							<!--/moderateform-->
							<!--pingbacks-->
								<p id="comments">$lang.pingbacks</p>
								<ol class="commentlist" start="1">
									<!--pingback-->
									<li class="alt" id="pingback-$pingback.id">
										<cite><a href="$pingback.url">$pingback.title</a></cite>
									</li>
									<!--/pingback-->
								</ol>
							<!--/pingbacks-->

							<!--closed-->
							<p class="nocomments">$lang.closed</p>
							<!--/closed-->
							
							<!--form-->
								<p id="respond">$lang.leavereply</p>
								<form action="$options.url/send-comment.php" method="post" id="commentform">
									<p><input type="text" name="name" id="name" value="$name" size="22" tabindex="1" />
									<label for="name">$lang.name</label></p>

									<p><input type="text" name="email" id="email" value="$email" size="22" tabindex="2" />
									<label for="email">$lang.email</label></p>

									<p><input type="text" name="url" id="url" value="$url" size="22" tabindex="3" />
									<label for="url">$lang.url</label></p>

									<p><input type="checkbox" name="subscribe" id="subscribe" $subscribe size="22" tabindex="4" />
									<label for="subscribe">$lang.subscribe</label></p>

									<p><textarea name="content" id="comment" rows="10" cols="60%" tabindex="5"></textarea></p>

									<input type="hidden" name="postid" value="$postid" />
									<input type="hidden" name="antispam" value="$antispam" />

									<p><input name="submit" type="submit" id="submit" tabindex="6" value="$lang.send" /></p>
								</form>
							<!--/form-->
							<!--confirmform-->
								<h2>$lang.formhead</h2>
								<form name="preform" method="post" action="">
								  <p><input type="submit" name="submit1" value="$lang.robot"/></p>
								</form>

								<form name="form" method="post" action="">
								<input type="hidden" name="confirmid" value="$confirmid" />
								  <p><input type="submit" name="submit2" value="$lang.human"/></p>
								</form>
							<!--/confirmform-->
						<!--/templatecomments-->
			</div>
		<!--/post-->

		<!--navi-->
			<div class="navigation">
			<!--prev--><span class="previous-entries">$link</span><!--/prev-->
			<!--next--><span class='"next-entries">$link</span><!--/next-->
			<!--link--><a href="$options.url$url">$page</a><!--/link-->
			<!--current-->$page<!--/current-->
			<!--divider--> | <!--/divider-->
			</div>
		<!--/navi-->
		<!--menu-->
			<div class="post" id="menu-$menu.id">
		        <h2>$menu.title</h2>
		        <div class="post-content">
					$menu.content
		        </div>
			</div>
		<!--/menu-->
		<!--admin-->
			<!--edit-->
			<input type="text" name="$name" id="$name" value="$value" size="22" />
			<!--/edit-->

			<!--area-->
			<textarea name="$name" id="$name" cols="57%" rows="10">$value</textarea>
			<!--/area-->

<!--checkbox-->
<p><input type="checkbox" name="$name" id="$name" $value />
<label for="$name"><strong>$lang.$name</strong></label></p>
<!--/checkbox-->

		<!--/admin-->
		<!--/content-->
	</div>
	<div id="sidebar">
		<!--sitebar-->
			<!--widget-->	
			<div class="widget">
		      <div class="rubric">$title</div>
    <!--items-->
		      <ul class="linkcat">
				      <!--item-->
          <li><a rel="$rel" href="$url" title="$title">$icon$anchor</a>$subitems</li>
          <!--/item-->
		      </ul>
    <!--/items-->
		    </div>
			<!--/widget-->

			<!--submenu-->
				<div class="widget">
			      <div class="rubric">$title</div>
    <!--items-->
			      <ul class="list-page">
				      <!--item-->
          <li><a rel="$rel" href="$url" title="$title">$icon$anchor</a>$subitems</li>
          <!--/item-->
			      </ul>
    <!--/items-->
				</div>
			<!--/submenu-->

			<!--categories-->
				<div class="widget">
			      <div class="rubric">$title</div>
    <!--items-->
			      <ul class="list-cat" id="widgetcategories">
					<!--item-->
					<li><a rel="$rel" href="$url" title="$title">$icon$anchor</a>$subitems</li>
					<!--/item-->
			      </ul>
    <!--/items-->
			    </div>
			<!--/categories-->

			<!--archives-->
				<div class="widget">
			      <div class="rubric">$title</div>
    <!--items-->
			      <ul class="list-archives" id="widgetarchives">
					<!--item-->
					<li><a href="$url" rel="archives" title="$title">$icon$title</a>$subitems</li>
					<!--/item-->
			      </ul>
    <!--/items-->
			    </div>
			<!--/archives-->

		<!--posts-->
				<div class="widget">
			      <div class="rubric">%s</div>
    <!--items-->
		      <ul class="linkcat">
					<!--item-->
						<li><a href="$post.link" rel="bookmark" title="$lang.permalink $post.title">$post.title</a></li>
					<!--/item-->
			    </ul>
    <!--/items-->
			</div>
		<!--/posts-->

			<!--comments-->	
			<div class="widget">
		      <div class="rubric">$title</div>
    <!--items-->
		      <ul class="linkcat">
				      <!--item-->
						<li><a href=" $options.url$posturl#comment-$id" title="$name $onrecent $title">$name $onrecent $title</a>: $content...</li>
          <!--/item-->
		      </ul>
    <!--/items-->
		    </div>
			<!--/comments-->

	<!--meta-->
			<div class="widget">
		      <div class="rubric">$title</div>
    <!--items-->
		      <ul class="linkcat">
				      <!--item-->
          <li><a rel="$rel" $class href="$url" title="$title">$icon$anchor</a>$subitems</li>
          <!--/item-->
    <!--metaclasses-->rss=rss,comments=rss-comments,foaf=foaf<!--/metaclasses-->
		      </ul>
    <!--/items-->
		    </div>
<!--/meta-->

		<!--/sitebar-->
	</div>
		
	<div id="footer">
	<div id="bottom">
	<ul id="footerbar">
		<!--sitebar-->

		<!--widget-->
			<div class="rubric">$title</div>
    <!--items-->
				<ul>
      <!--item-->
      <li><a rel="$rel" href="$url" title="$title">$icon$anchor</a>$subitems</li>
      <!--/item-->
				</ul>
    <!--/items-->
			<!--/widget-->

		<!--posts-->
		<li id="lastposts">
			<div class="rubric">%s</div>
    <!--items-->
				<ul id="widgetposts">
					<!--item-->
						<li>
							<a href="$post.link" rel="bookmark" title="$lang.permalink $post.title">$post.title</a><br />
							<small>$post.date</small>
						</li>
					<!--/item-->
			    </ul>
    <!--/items-->
			</li>
		<!--/posts-->

		<!--comments-->
		<li id="lastcomments">
			<div class="rubric">$title</div>
    <!--items-->
				<ul id="widgetcomments">
					<!--item-->
						<li><a href=" $options.url$posturl#comment-$id" title="$name $onrecent $title">$name $onrecent $title</a>: $content...</li>
					<!--/item-->
				</ul>
    <!--/items-->
		</li>
		<!--/comments-->

	<!--meta-->
	<li id="meta">
				<div class="rubric">$title</div>
    <!--items-->
				<ul id="widgetmeta">
				      <!--item-->
          <li><a rel="$rel" $class href="$url" title="$title">$icon$anchor</a>$subitems</li>
          <!--/item-->
    <!--metaclasses-->rss=rss,comments=rss-comments,foaf=foaf<!--/metaclasses-->
				</ul>
    <!--/items-->
			</li>
<!--/meta-->
<!--/sitebar-->
	</ul>
		
			<div id="copyright"><span class="blogolet"><a href="$options.url$options.home">$options.name</a></span>$template.footer</div>
	</div>
</div>
</div>
</div>	
</body>
</html>